{
  "name": "cra-template-pdr-ts-inputon",
  "version": "1.0.2",
  "private": false,
  "license": "MIT",
  "author": "Pedro Vasconcellos <pedrovasalmeida@gmail.com>",
  "description": "A quick start Create React App template with React Router, TypeScript and custom EditorConfig, ESlint and Prettier configurations",
  "keywords": [
    "react",
    "create-react-app",
    "cra-template",
    "template",
    "eslint",
    "prettier",
    "editor-config",
    "react-router",
    "react-router-dom",
    "history",
    "typescript"
  ],
  "files": [
    "template",
    "template.json"
  ],
  "clean-files": "rm -rf ./template/public ./template/src && rm -f ./template/.eslintrc",
  "copy-files": "cp -a ./src/. template/src && cp -a ./public/. template/public && cp .eslintrc template/",
  "prepublishOnly": "yarn clean-files && yarn copy-files",
  "repository": {
    "type": "git",
    "url": "https://github.com/pedrovasalmeida/pdr-ts-inputon",
    "directory": "/"
  },
  "homepage": "https://inputon.com.br",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "history": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "styled-components": "^5.2.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
